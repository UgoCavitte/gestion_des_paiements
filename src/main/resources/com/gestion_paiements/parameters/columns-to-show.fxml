<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="800.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gestion_paiements.controllers.parameters.ColumnsToShowController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Tableau compte en banque" />
            <HBox>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <children>
                  <CheckBox fx:id="BAId" mnemonicParsing="false" onAction="#BAId" text="ID">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="BADateSent" mnemonicParsing="false" onAction="#BADateSent" text="Date d'envoi">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="BADateReceived" mnemonicParsing="false" onAction="#BADateReceived" text="Date de réception">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="BAAmountSent" mnemonicParsing="false" onAction="#BAAmountSent" text="Somme envoyée">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="BAAmountReceived" mnemonicParsing="false" onAction="#BAAmountReceived" text="Somme reçue">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="BASender" mnemonicParsing="false" onAction="#BASender" text="Envoyeur">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="BAProducts" mnemonicParsing="false" onAction="#BAProducts" text="Produits">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="BAComment" mnemonicParsing="false" onAction="#BAComment" text="Commentaire">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </CheckBox>
               </children>
            </HBox>
            <Label layoutX="20.0" layoutY="20.0" text="Tableau plateforme" />
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox>
                     <children>
                        <CheckBox fx:id="PSentToBank" layoutX="15.0" layoutY="15.0" mnemonicParsing="false" onAction="#PSentToBank" text="Envoyé ?">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin></CheckBox>
                        <CheckBox fx:id="PId" mnemonicParsing="false" onAction="#PId" text="ID">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="PDateSent" mnemonicParsing="false" onAction="#PDateSent" text="Date d'envoi">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="PDateReceived" mnemonicParsing="false" onAction="#PDateReceived" text="Date de réception">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="PAmountSent" mnemonicParsing="false" onAction="#PAmountSent" text="Somme envoyée">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="PAmountReceived" mnemonicParsing="false" onAction="#PAmountReceived" text="Somme reçue">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="10.0">
                     <children>
                        <CheckBox fx:id="PSender" mnemonicParsing="false" onAction="#PSender" text="Envoyeur">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="PProducts" mnemonicParsing="false" onAction="#PProducts" text="Produits">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="PIDEnvoiCompte" mnemonicParsing="false" onAction="#PIDEnvoiCompte" text="ID d'envoi">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                        <CheckBox fx:id="PComment" mnemonicParsing="false" onAction="#PComment" text="Commentaire">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </CheckBox>
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
